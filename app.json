{
  "expo": {
    "name": "recontrole",
    "slug": "recontrole",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/app_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lukaspio.recontrole"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/app_logo.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "23dfdcec-dc4d-4f23-b98c-bcb1eb1b2067"
      }
    }
  }
}
